import sys

def main():
    ans = None
    default_temp = 15.0
    input_received = False

    try:
        lines = sys.stdin.readlines()
    except EOFError:
        sys.exit(1)

    if not lines or all(not line.strip() for line in lines):
        sys.exit(1)

    try:
        for line in lines:
            parts = line.split()
            if not parts:
                continue

            seconds = float(parts[0])
            input_received = True

            if len(parts) > 1:
                temp = float(parts[1])
            else:
                temp = default_temp

            ans = (331.5 + 0.6 * temp) * seconds

        if not input_received:
            sys.exit(1)

        if ans is not None:
            print(ans)

    except ValueError:
        sys.exit(1)

if __name__ == "__main__":
    main()
